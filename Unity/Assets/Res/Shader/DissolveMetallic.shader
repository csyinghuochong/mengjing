				float3 WorldViewDirection = _WorldSpaceCameraPos.xyz  - WorldPosition;
				float4 ShadowCoords = float4( 0, 0, 0, 0 );
				#if defined(ASE_NEEDS_FRAG_SCREEN_POSITION)
				float4 ScreenPos = IN.screenPos;
				#endif

				#if defined(REQUIRES_VERTEX_SHADOW_COORD_INTERPOLATOR)
					ShadowCoords = IN.shadowCoord;
				#elif defined(MAIN_LIGHT_CALCULATE_SHADOWS)
					ShadowCoords = TransformWorldToShadowCoord( WorldPosition );
				#endif
	
				WorldViewDirection = SafeNormalize( WorldViewDirection );

				float2 uv_Albedo7 = IN.ase_texcoord7.xy;
				
				float2 uv_NormalTexture44 = IN.ase_texcoord7.xy;
				
				float2 uv_GuideTexture = IN.ase_texcoord7.xy * _GuideTexture_ST.xy + _GuideTexture_ST.zw;
				float temp_output_59_0 = ( tex2D( _GuideTexture, uv_GuideTexture ).r * _GuideAdjuster );
				float DissolveAmount39 = _DissolveAmount;
				float4 lerpResult24 = lerp( float4( 0,0,0,0 ) , _DissolveColor , step( temp_output_59_0 , DissolveAmount39 ));
				float2 uv_EmissionTexture49 = IN.ase_texcoord7.xy;
				
				float2 uv_MetallicTexture56 = IN.ase_texcoord7.xy;
				
				float2 uv_OcclusionMap54 = IN.ase_texcoord7.xy;
				
				float3 Albedo = ( tex2D( _Albedo, uv_Albedo7 ) * _Color ).rgb;
				float3 Normal = UnpackNormalScale( tex2D( _NormalTexture, uv_NormalTexture44 ), 1.0f );
				float3 Emission = ( lerpResult24 + ( tex2D( _EmissionTexture, uv_EmissionTexture49 ) * _EmissionColor ) ).rgb;
				float3 Specular = 0.5;
				float Metallic = ( tex2D( _MetallicTexture, uv_MetallicTexture56 ).r * _MetallicValue );
				float Smoothness = _SmoothnessValue;
				float Occlusion = tex2D( _OcclusionMap, uv_OcclusionMap54 ).r;
				float Alpha = ( temp_output_59_0 <= (0.0 + (( DissolveAmount39 - _EdgeWidth ) - 0.0) * (1.0 - 0.0) / (( 1.0 - _EdgeWidth ) - 0.0)) ? 0.0 : 1.0 );
				float AlphaClipThreshold = 0.5;
				float AlphaClipThresholdShadow = 0.5;
				float3 BakedGI = 0;
				float3 RefractionColor = 1;
				float RefractionIndex = 1;
				float3 Transmission = 1;
				float3 Translucency = 1;
				#ifdef ASE_DEPTH_WRITE_ON
