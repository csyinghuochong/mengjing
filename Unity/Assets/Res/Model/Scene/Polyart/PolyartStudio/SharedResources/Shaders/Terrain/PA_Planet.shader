				float2 texCoord48 = IN.ase_texcoord3.xy * float2( 1,1 ) + float2( 0,0 );
				float2 panner6 = ( 1.0 * _Time.y * appendResult5 + texCoord48);
				float2 appendResult17 = (float2(_PlanetSpeedX , 0.0));
				float2 texCoord47 = IN.ase_texcoord3.xy * float2( 1,1 ) + float2( 0,0 );
